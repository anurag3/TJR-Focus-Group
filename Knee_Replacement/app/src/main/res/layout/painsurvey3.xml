<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".goToPainSurvey3">

<ImageView
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/imageView2"
    android:src="@drawable/bodyoutline"
    />


<Button
    android:layout_width="25dp"
    android:layout_height="25dp"
    android:id="@+id/bodybtn1"
    android:background="@drawable/roundbutton"
    android:layout_centerVertical="true"
    android:layout_centerHorizontal="true"
    android:onClick="buttonselection3"
    android:padding="50dp" />

<Button
    android:layout_width="25dp"
    android:layout_height="25dp"
    android:id="@+id/bodybtn2"
    android:onClick="buttonselection3"
    android:background="@drawable/roundbutton"
    android:layout_marginEnd="7dp"
    android:layout_marginTop="17dp"
    android:layout_below="@+id/bodybtn1"
    android:padding="50dp"
    android:layout_toStartOf="@+id/bodybtn1" />

    <Button
    android:layout_width="25dp"
    android:layout_height="25dp"
    android:id="@+id/bodybtn4"
    android:onClick="buttonselection3"
    android:background="@drawable/roundbutton"
    android:layout_marginTop="38dp"
    android:layout_below="@+id/bodybtn2"
    android:layout_alignEnd="@+id/bodybtn2"
    android:layout_marginEnd="2dp"
    android:padding="50dp"/>

    <Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Next"
    android:id="@+id/interface3next"
    android:layout_alignParentBottom="true"
    android:layout_alignParentEnd="true"
    android:textStyle="bold"
    android:onClick="onClick"
    android:visibility="visible" />


</RelativeLayout>