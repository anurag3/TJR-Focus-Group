<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="60dp"
        android:layout_height="343dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/face_vertical"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp">

        <Button
            android:onClick="onface"
            android:tag="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text=""
            android:id="@+id/button1"
            android:layout_weight="1" />

        <Button
            android:onClick="onface"
            android:tag="2"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text=""
            android:id="@+id/button2"
            android:layout_weight="1" />

        <Button
            android:onClick="onface"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text=""
            android:id="@+id/button3"
            android:tag="3"
            android:layout_weight="1" />

        <Button
            android:onClick="onface"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text=""
            android:tag="4"
            android:id="@+id/button4"
            android:layout_weight="1" />

        <Button
            android:onClick="onface"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text=""
            android:tag="5"
            android:id="@+id/button5"
            android:layout_weight="1" />

        <Button
            android:onClick="onface"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text=""
            android:tag="6"
            android:id="@+id/button6"
            android:layout_weight="1" />
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="150dp"
        android:layout_height="343dp"
        android:layout_marginLeft="10dp" >

        <RelativeLayout
            android:clickable="true"
            android:onClick="onface"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:tag="1"
            android:gravity="center" >

            <TextView
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Hurts Worst"
                android:tag="1"
                android:onClick="onface"
                android:id="@+id/textView1" />
        </RelativeLayout>

        <RelativeLayout
            android:clickable="true"
            android:onClick="onface"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:tag="2"
            android:gravity="center" >

            <TextView
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Hurts Whole Lot"
                android:tag="2"
                android:onClick="onface"
                android:id="@+id/textView2" />
        </RelativeLayout>

        <RelativeLayout
            android:clickable="true"
            android:onClick="onface"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:tag="3"
            android:gravity="center" >

            <TextView
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="onface"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Hurts Even More"
                android:tag="3"
                android:id="@+id/textView3" />
        </RelativeLayout>

        <RelativeLayout
            android:clickable="true"
            android:onClick="onface"
            android:tag="4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center" >

            <TextView
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:tag="4"
                android:onClick="onface"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Hurts Little More"
                android:id="@+id/textView4" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:tag="2"
            android:onClick="onface"
            android:gravity="center" >

            <TextView
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Hurts a Little Bit"
                android:tag="5"
                android:onClick="onface"
                android:id="@+id/textView5" />
        </RelativeLayout>

        <RelativeLayout
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="onface"
            android:tag="6"
            android:gravity="center" >

            <TextView
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="onface"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="No Hurt"
                android:tag="6"
                android:id="@+id/textView6" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="343dp">

    <com.android.widget.VerticalSeekBar
        android:id="@+id/VerticalSeekBar1"
        android:layout_width="fill_parent"
        android:layout_height="343dp"
        android:scrollIndicators="start|end" />
</LinearLayout>
</LinearLayout>